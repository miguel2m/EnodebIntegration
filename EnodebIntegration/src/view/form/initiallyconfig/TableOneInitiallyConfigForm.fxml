<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane hgap="10.0" vgap="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <Label text="NE" />
            <Label text="Ethernet Port Number" GridPane.rowIndex="1" />
            <Label text="Interface IP Address" GridPane.rowIndex="2" />
            <Label text="Mask" GridPane.rowIndex="3" />
            <Label text="Next-Hop IP Address (M2000 | S-GW | MME)" GridPane.rowIndex="4" />
            <Label text="Next-Hop VLAN (U2000 | S-GW | MME)" GridPane.rowIndex="5" />
            <TextField fx:id="neEnodebTableOne" promptText="NE" GridPane.columnIndex="1" />
            <TextField fx:id="ethernetPortNumberEnodebTableOne" promptText="Ethernet Port Number" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="interfaceIpAddEnodebTableOne" promptText="Interface IP Address" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="maskEnodebTableOne" promptText="Mask" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="nexthopIpAddEnodebTableOne" promptText="Next-Hop IP Address (M2000 | S-GW | MME)" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField fx:id="nexthopVlanAddEnodebTableOne" promptText="Next-Hop VLAN (U2000 | S-GW | MME)" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <HBox spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="6">
               <children>
                  <Button mnemonicParsing="false" onAction="#onFormTableOneInitConfig" text="Aceptar" textFill="#0013ff" />
